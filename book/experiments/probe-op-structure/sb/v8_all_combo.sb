(version 1)
(deny default)
(allow file-read*
  (require-any
    (subpath "/tmp/foo")
    (literal "/etc/hosts")
    (vnode-type REGULAR-FILE)))
(allow mach-lookup
  (global-name "com.apple.cfprefsd.agent"))
(allow network-outbound
  (require-all
    (socket-domain AF_INET)
    (socket-type SOCK_STREAM)
    (socket-protocol IPPROTO_TCP)))
(allow iokit-open*
  (iokit-registry-entry-class "IOUSBHostInterface"))
