(version 1)
(deny default)
(allow file-read* (require-any
  (literal "/etc/hosts")
  (literal "/tmp/foo")
  (literal "/usr/bin/yes")
  (literal "/var/log/system.log")
  (literal "/Applications/Calculator.app")
  (literal "/System/Library/Fonts/Supplemental/Arial.ttf")
  (literal "/private/var/db/DetachedSignatures")))
