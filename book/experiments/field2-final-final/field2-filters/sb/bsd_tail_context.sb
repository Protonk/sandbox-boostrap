(version 1)
(allow default)
;; Mimic bsd tail context with dtracehelper and posix_spawn literals plus an extra mach rule
(deny file-read* (literal "/dev/dtracehelper"))
(allow file-read* (literal "/usr/share/posix_spawn_filtering_rules"))
(allow mach-lookup (global-name "com.apple.test.dtracehelper"))
(deny file-write* (literal "/dev/dtracehelper"))
