{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "noEmitOnError": true,
    "sourceMap": false,
    "declaration": false,
    "pretty": false,
    "newLine": "lf",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "*.ts"
  ]
}

