{
  "expected_matrix": {
    "profiles": {
      "bucket4:v1_read": {
        "blob": "book/experiments/op-table-operation/sb/v1_read.sb",
        "mode": "sbpl",
        "notes": "Baseline bucket-4 profile: allow file-read* only.",
        "probes": [
          {
            "expected": "allow",
            "name": "read_/etc/hosts",
            "operation": "file-read*",
            "target": "/etc/hosts"
          },
          {
            "expected": "deny",
            "name": "write_/etc/hosts",
            "operation": "file-write*",
            "target": "/etc/hosts"
          },
          {
            "expected": "allow",
            "name": "read_/tmp/foo",
            "operation": "file-read*",
            "target": "/tmp/foo"
          }
        ]
      },
      "bucket5:v11_read_subpath": {
        "blob": "book/experiments/op-table-operation/sb/v11_read_subpath.sb",
        "mode": "sbpl",
        "notes": "Bucket-5 subpath profile: allow read under /tmp/foo; deny other reads/writes.",
        "probes": [
          {
            "expected": "allow",
            "name": "read_/tmp/foo",
            "operation": "file-read*",
            "target": "/tmp/foo"
          },
          {
            "expected": "deny",
            "name": "read_/tmp/bar",
            "operation": "file-read*",
            "target": "/tmp/bar"
          },
          {
            "expected": "deny",
            "name": "write_/tmp/foo",
            "operation": "file-write*",
            "target": "/tmp/foo"
          }
        ]
      },
      "runtime:allow_all": {
        "blob": "book/profiles/golden-triple/allow_all.sb.bin",
        "mode": "blob",
        "notes": "allow default; should allow all file read/write",
        "probes": [
          {
            "expectation_id": "allow_all|file-read*|/etc/hosts",
            "expected": "allow",
            "name": "read_/etc/hosts",
            "operation": "file-read*",
            "target": "/etc/hosts"
          },
          {
            "expectation_id": "allow_all|file-read*|/tmp/sbpl_rt/read.txt",
            "expected": "allow",
            "name": "read_/tmp/sbpl_rt/read.txt",
            "operation": "file-read*",
            "target": "/tmp/sbpl_rt/read.txt"
          },
          {
            "expectation_id": "allow_all|file-write*|/tmp/sbpl_rt/write.txt",
            "expected": "allow",
            "name": "write_/tmp/sbpl_rt/write.txt",
            "operation": "file-write*",
            "target": "/tmp/sbpl_rt/write.txt"
          }
        ]
      },
      "runtime:metafilter_any": {
        "blob": "book/experiments/sbpl-graph-runtime/profiles/metafilter_any.sb",
        "mode": "sbpl",
        "notes": "require-any literal foo/bar; deny baz.",
        "probes": [
          {
            "expectation_id": "metafilter_any|file-read*|/tmp/foo.txt",
            "expected": "allow",
            "name": "read_foo",
            "operation": "file-read*",
            "target": "/tmp/foo.txt"
          },
          {
            "expectation_id": "metafilter_any|file-read*|/tmp/bar.txt",
            "expected": "allow",
            "name": "read_bar",
            "operation": "file-read*",
            "target": "/tmp/bar.txt"
          },
          {
            "expectation_id": "metafilter_any|file-read*|/tmp/qux.txt",
            "expected": "allow",
            "name": "read_other",
            "operation": "file-read*",
            "target": "/tmp/qux.txt"
          },
          {
            "expectation_id": "metafilter_any|file-read*|/tmp/baz.txt",
            "expected": "deny",
            "name": "read_baz",
            "operation": "file-read*",
            "target": "/tmp/baz.txt"
          },
          {
            "expectation_id": "metafilter_any|file-write*|/tmp/baz.txt",
            "expected": "deny",
            "name": "write_baz",
            "operation": "file-write*",
            "target": "/tmp/baz.txt"
          }
        ]
      },
      "runtime:strict_1": {
        "blob": "book/experiments/runtime-checks/strict_1.sb",
        "mode": "sbpl",
        "notes": "Strict deny-default profile with minimal allow-set for helper liveness.",
