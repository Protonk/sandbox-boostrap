(version 2)
(deny default)
(allow iokit-open-service
  (iokit-registry-entry-class "IOSurfaceRoot"))
(allow iokit-open-user-client
  (iokit-user-client-class "IOSurfaceRootUserClient"))
(allow iokit-open
  (iokit-user-client-class "IOSurfaceRootUserClient")
  (apply-message-filter
    (deny iokit-external-method)))
